lineas = list()
print("Ingrese lineas para ser retornadas en mayusculas. Para imprimir el resultado ingrese una linea en blanco.")
string = "algo"
while (string != ""):
    string = raw_input("Escriba una linea: ")
    if (string != ""):
        lineas.append(string)
for i in range(0, len(lineas)):
    lineas[i] = lineas[i].upper()
print(lineas)

